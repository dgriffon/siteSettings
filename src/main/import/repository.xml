<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <siteSettings j:dependencies="default"
                    j:installedModules="siteSettings"
                    j:modulePriority="0"
                    j:moduleType="module"
                    j:title="Jahia Site Settings"
                    jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <text_rich_colored.png j:height="16"
                                      j:width="16"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </text_rich_colored.png>
                        <briefcase_document.png j:height="16"
                                       j:width="16"
                                       jcr:mixinTypes="jmix:image"
                                       jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </briefcase_document.png>
                        <link.png j:height="16"
                         j:width="16"
                         jcr:mixinTypes="jmix:image"
                         jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </link.png>
                        <signpost.png j:height="16"
                             j:width="16"
                             jcr:mixinTypes="jmix:image"
                             jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </signpost.png>
                        <urlmapping_large.png j:height="32"
                                     j:width="32"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </urlmapping_large.png>
                        <users_into.png j:height="16"
                               j:width="16"
                               jcr:mixinTypes="jmix:image"
                               jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </users_into.png>
                        <link_large.png j:height="32"
                               j:width="32"
                               jcr:mixinTypes="jmix:image"
                               jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </link_large.png>
                        <text_rich_colored_large.png j:height="32"
                                            j:width="32"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </text_rich_colored_large.png>
                        <signpost_large.png j:height="32"
                                   j:width="32"
                                   jcr:mixinTypes="jmix:image"
                                   jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </signpost_large.png>
                        <briefcase_document_large.png j:height="32"
                                             j:width="32"
                                             jcr:mixinTypes="jmix:image"
                                             jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </briefcase_document_large.png>
                        <users_into_large.png j:height="32"
                                     j:width="32"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </users_into_large.png>
                        <urlmapping.png j:height="16"
                               j:width="16"
                               jcr:mixinTypes="jmix:image"
                               jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </urlmapping.png>
                        <manage_users.png j:height="16"
                                 j:width="16"
                                 jcr:mixinTypes="jmix:image"
                                 jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </manage_users.png>
                        <page_models_large.png j:height="32"
                                      j:width="32"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </page_models_large.png>
                        <page_models.png j:height="16"
                                j:width="16"
                                jcr:mixinTypes="jmix:image"
                                jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </page_models.png>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <site-settings-base j:view="siteSettings"
                                jcr:mixinTypes="jmix:useUILocale"
                                jcr:primaryType="jnt:template">
                        <manageLanguages j:applyOn="jnt:virtualsite"
                                j:defaultTemplate="false"
                                j:hiddenTemplate="true"
                                j:icon="/modules/siteSettings/templates/files/signpost.png"
                                j:priority="100"
                                j:requiredPermissionNames="siteAdminLanguages"
                                j:titleKey="siteSettings.label.manageLanguages"
                                jcr:mixinTypes="jmix:rbTitle"
                                jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <manage-languages jcr:primaryType="jnt:siteSettingsManageLanguages"/>
                            </pagecontent>
                        </manageLanguages>
                        <wcagCompliance j:applyOn="jnt:virtualsite"
                               j:defaultTemplate="false"
                               j:hiddenTemplate="true"
                               j:icon="/modules/siteSettings/templates/files/text_rich_colored.png"
                               j:priority="100"
                               j:requiredPermissionNames="siteAdminWcagCompliance"
                               j:titleKey="siteSettings.label.wcagCompliance"
                               jcr:mixinTypes="jmix:rbTitle"
                               jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <wcag-compliance jcr:primaryType="jnt:siteSettingsWcagCompliance"/>
                            </pagecontent>
                        </wcagCompliance>
                        <htmlFiltering j:applyOn="jnt:virtualsite"
                              j:defaultTemplate="false"
                              j:hiddenTemplate="true"
                              j:icon="/modules/siteSettings/templates/files/text_rich_colored.png"
                              j:priority="100"
                              j:requiredPermissionNames="siteAdminHtmlSettings"
                              j:titleKey="siteSettings.label.htmlFiltering"
                              jcr:mixinTypes="jmix:rbTitle"
                              jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <html-filtering jcr:primaryType="jnt:siteSettingsHtmlFiltering"/>
                            </pagecontent>
                        </htmlFiltering>
                        <vanityURLSetting j:applyOn="jnt:virtualsite"
                                 j:defaultTemplate="false"
                                 j:hiddenTemplate="true"
                                 j:icon="/modules/siteSettings/templates/files/urlmapping.png"
                                 j:priority="100"
                                 j:requiredPermissionNames="siteAdminUrlmapping"
                                 j:titleKey="siteSettings.label.vanityURLSettings"
                                 jcr:mixinTypes="jmix:rbTitle"
                                 jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <vanityurlslist jcr:mixinTypes="jmix:accessControlled"
                                     jcr:primaryType="jnt:siteSettingsVanityUrlsList">
                                    <j:acl jcr:primaryType="jnt:acl"/>
                                </vanityurlslist>
                            </pagecontent>
                        </vanityURLSetting>
                        <manageModules j:applyOn="jnt:virtualsite"
                              j:defaultTemplate="false"
                              j:hiddenTemplate="true"
                              j:icon="/modules/siteSettings/templates/files/briefcase_document.png"
                              j:priority="100"
                              j:requiredPermissionNames="siteAdminTemplates"
                              j:titleKey="siteSettings.label.manageModules"
                              jcr:mixinTypes="jmix:rbTitle"
                              jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <manage-modules jcr:primaryType="jnt:siteSettingsManageModules"/>
                            </pagecontent>
                        </manageModules>
                        <manageUsers j:applyOn="jnt:virtualsite"
                            j:defaultTemplate="false"
                            j:hiddenTemplate="true"
                            j:icon="/modules/siteSettings/templates/files/manage_users.png"
                            j:priority="100"
                            j:requiredPermissionNames="siteAdminUsers"
                            j:titleKey="siteSettings.manageUsers"
                            jcr:mixinTypes="jmix:rbTitle"
                            jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <manage-users jcr:primaryType="jnt:siteSettingsManageUsers"/>
                            </pagecontent>
                        </manageUsers>
                        <manageGroups j:applyOn="jnt:virtualsite"
                             j:defaultTemplate="false"
                             j:hiddenTemplate="true"
                             j:icon="/modules/siteSettings/templates/files/users_into.png"
                             j:priority="100"
                             j:requiredPermissionNames="siteAdminGroups"
                             j:titleKey="siteSettings.label.manageGroups"
                             jcr:mixinTypes="jmix:rbTitle"
                             jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <manage-groups jcr:primaryType="jnt:siteSettingsManageGroups"/>
                            </pagecontent>
                        </manageGroups>
                        <manageSiteRoles j:applyOn="jnt:virtualsite"
                                j:defaultTemplate="false"
                                j:hiddenTemplate="true"
                                j:icon="/modules/siteSettings/templates/files/users_into.png"
                                j:priority="100"
                                j:requiredPermissionNames="siteAdminSiteRoles"
                                j:titleKey="siteSettings.manageSiteRoles"
                                jcr:mixinTypes="jmix:rbTitle"
                                jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <manage-site-roles jcr:primaryType="jnt:manageRoles" roleGroup="site-role"/>
                            </pagecontent>
                        </manageSiteRoles>
                        <page-models j:applyOn="jnt:virtualsite"
                            j:icon="/modules/siteSettings/templates/files/page_models.png"
                            jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Page Models"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <sitesettingsmanagepagemodels jcr:primaryType="jnt:siteSettingsManagePageModels"/>
                            </pagecontent>
                        </page-models>
                    </site-settings-base>
                </templates>
                
            </siteSettings>
    </modules>
</content>